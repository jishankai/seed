@charset "utf-8";
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, form, fieldset, input, textarea, p, blockquote, th, td, center, header, menu, section, article{
	margin:0; padding:0;
}
h1, h2, h3, h4, h5, h6{ font-size:12px; }
body{
	font-size:24px; font-family:migmix,Arial,Helvetica,sans-serif; height:100%; color:#000; background:transparent;
	-webkit-text-size-adjust:none; -webkit-user-select:none; -webkit-tap-highlight-color:rgba(0,0,0,0);
}
table{ border-collapse:collapse; border-spacing:0; }
samp{ font-family:migmix,Arial,Helvetica,sans-serif; }
ol, ul, li, menu, nav{ list-style:none; }
a{ text-decoration:none; }
img{ border:none; }
.fr { float:right;}

/*半透明*/
.op05 {opacity: 0.5}
.op08 {opacity: 0.8}

/*clear*/
.clear{ clear:both; }
.wrapper{ width:640px; position:absolute; left:50%; top:0; margin-left:-320px; overflow:hidden; min-height:862px; background:rgba(0,0,0,0); }
.clearfix:after,
.wrapper:after{ visibility:hidden; display:block; height:0; line-height:0; clear:both; content:""; }

